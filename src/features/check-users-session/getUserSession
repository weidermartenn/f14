import { supabase } from "@/shared/api/supabaseClient.ts"

export const getUserSession = async()=> {
    try {
        const {data, error} = await supabase.auth.getSession();

        if (error) {
            throw error;
        }

        return data.session;
    } catch (error) {
        console.error("Ошибка при получении сессии: ", error);
        return null;
    }
}